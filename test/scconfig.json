{
  "env": "testnet",
  "main": "@scom/dapp",
  "assets": "@modules/assets",
  "bundle": true,
  "moduleDir": "modules",
  "modules": {
    "@modules/assets": {
      "path": "assets"
    },
    "@modules/module1": {
      "path": "module1"
    }
  },
  "dependencies": {
    "@scom/dapp": "*",
    "@scom/scom-designer": "*",
    "@ijstech/compiler": "*",
    "@scom/scom-chart-data-source-setup": "*",
    "@scom/scom-line-chart": "*",
    "@scom/scom-bar-chart": "*",
    "@scom/scom-pie-chart": "*",
    "@scom/scom-scatter-chart": "*",
    "@scom/scom-mixed-chart": "*",
    "@scom/scom-area-chart": "*"
  },
  "packages": [    
    "@scom/scom-table"
  ],
  "menus": [
  ],
  "routes": [
    {
      "url": "/module1",
      "module": "@modules/module1",
      "default": true
    }
  ],
  "themes": {
    "default": "dark",
    "dark": {
      "background": {
        "default": "#212128",
        "paper": "#000000",
        "main": "linear-gradient(270deg, #FF9900 0%, #FC7428 100%)",
        "modal": "#1A1A1A"
      },
      "text": {
        "secondary": "hsla(0, 0%, 100%, 0.6)"
      },
      "divider": "#FFFFFF",
      "typography": {
        "fontFamily": "Raleway"
      }
    },
    "light": {}
  },
  "copyrightInfo": "&#169; 2022 IJS Technologies. All rights reserved.",
  "version": "0.1.0-beta",
  "breakpoints": {
    "mobile": 414,
    "tablet": 1280,
    "desktop": 1920
  }
}